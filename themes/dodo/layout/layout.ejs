<!DOCTYPE html>
<html>
<%- partial('_partial/head') %>
<body>
<div class="container-fluid container-app">
    <div class="mask">
        <img class="pop-img pop-img-qq" src="<%- theme.qq %>" alt="">
        <img class="pop-img pop-img-wechat" src="<%- theme.wechat %>" alt="">
    </div>
    <div id="menu-icon-wrapper" class="menu-icon-wrapper" style="visibility: hidden">
        <svg width="1000px" height="1000px">
            <path id="pathA" d="M 300 400 L 700 400 C 900 400 900 750 600 850 A 400 400 0 0 1 200 200 L 800 800"></path>
            <path id="pathB" d="M 300 500 L 700 500"></path>
            <path id="pathC" d="M 700 600 L 300 600 C 100 600 100 200 400 150 A 400 380 0 1 1 200 800 L 800 200"></path>
        </svg>
        <button id="menu-icon-trigger" class="menu-icon-trigger"></button>
    </div>
    <div class="container-content">
        <%- partial('_partial/sidenav', null) %>
        <div class="main-content">
            <div class="main-content-scroll">
                <section><%- body %></section>
                <%- partial('_partial/footer', null, {cache: true}) %>
            </div>
        </div>
    </div>
</div>
</body>
<%- partial('_partial/scripts') %>
</html>
